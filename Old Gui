local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("[BETA]Remorse Hub", "BloodTheme")

-- MAIN
local Tab = Window:NewTab("Random Things")
local MainSection = Tab:NewSection("Random Things I Got")


MainSection:NewButton("CrimSkid", "The Worst pc/mobile Lock He Created :3", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/AlexTheScriptKiddie/dahood/main/modded"))()
end)

